<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:ObjectProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#readBadge">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#ScanningTask"/>
    <rdfs:range rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#Badge"/>
    <rdfs:comment>A task when a badge read</rdfs:comment>
    <rdfs:label>readBadge</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#usedTurnstileReader">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#ScanningTask"/>
    <rdfs:range rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:comment>The used Turnstile Reader for a given scanning task</rdfs:comment>
    <rdfs:label>usedTurnstileReader</rdfs:label>
  </owl:ObjectProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasReaderCode">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Reader Code of TurnstileReader</rdfs:comment>
    <rdfs:label>hasReaderCode</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasActivationDate">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#Badge"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:comment>Reader activation date</rdfs:comment>
    <rdfs:label>hasReaderActivationDate</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasReaderType">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Use of the Reader, e.g., presence detection or canteen access.</rdfs:comment>
    <rdfs:label>hasReaderType</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasReaderHead">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Has unique Reader identifier used in timestamp XML.</rdfs:comment>
    <rdfs:label>hasReaderHead</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasDataConcentratorServer">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Server hosted in CED that collects data from remote readers</rdfs:comment>
    <rdfs:label>hasDataConcentratorServer</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasRegion">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Geographical region where the reader is installed</rdfs:comment>
    <rdfs:label>hasDataConcentratorServer</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasTimestamp">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#ScanningTask"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#Badge"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
    <rdfs:comment>Unique timestamp</rdfs:comment>
    <rdfs:label>hasTimestampIdentifier</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasTimestampDirection">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#ScanningTask"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Direction of the timestamp, left to right or right to left</rdfs:comment>
    <rdfs:label>hasTimestampDirection</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#badgeEnabled">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#ScanningTask"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment>Boolean value for storing if the badge enabled from the turnstile reader</rdfs:comment>
    <rdfs:label>badgeEnabled</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasBadgeIdentifier">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#Badge"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Identifier Badge</rdfs:comment>
    <rdfs:label>hasBadgeIdentifier</rdfs:label>
  </owl:DataProperty>

  <owl:DataProperty rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#hasBadgeType">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    <rdfs:domain rdf:resource="https://glaciation-project.eu/MetadataReferenceModel/uc1#Badge"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Badge Type</rdfs:comment>
    <rdfs:label>hasBadgeType</rdfs:label>
  </owl:DataProperty>

  <owl:Class rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#TurnstileReader">
    <rdfs:subClassOf rdf:resource="https://glaciation-project.eu/MetadataReferenceModel#Resource"/>
    <rdfs:comment>A turnstile reader</rdfs:comment>
    <rdfs:label>TurnstileReader</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#Badge">
    <rdfs:subClassOf rdf:resource="https://glaciation-project.eu/MetadataReferenceModel#Resource"/>
    <rdfs:comment>A badge for being read from a turnstile reader</rdfs:comment>
    <rdfs:label>Badge</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://glaciation-project.eu/MetadataReferenceModel/uc1#ScanningTask">
    <rdfs:subClassOf rdf:resource="https://glaciation-project.eu/MetadataReferenceModel#AssignedTask"/>
    <rdfs:comment>A scanning task where a badge is read from a turnstile reader</rdfs:comment>
    <rdfs:label>ScanningTask</rdfs:label>
  </owl:Class>

</rdf:RDF>
